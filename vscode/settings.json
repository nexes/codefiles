{
    "telemetry.telemetryLevel": "error",
    "workbench.tree.indent": 16,
    "workbench.colorTheme": "Darcula Contrast",
    "workbench.startupEditor": "none",
    "workbench.sideBar.location": "left",
    "workbench.activityBar.location": "hidden",
    "workbench.editor.restoreViewState": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-dark",
    "editor.folding": false,
    "editor.fontSize": 14,
    "editor.lineHeight": 1.6,
    "editor.wordWrapColumn": 90,
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.hover.enabled": false,
    "editor.matchBrackets": "near",
    "editor.cursorBlinking": "solid",
    "editor.lightbulb.enabled": "off",
    "editor.detectIndentation": false,
    "editor.guides.indentation": false,
    "editor.trimAutoWhitespace": false,
    "editor.snippetSuggestions": "none",
    "editor.overviewRulerBorder": false,
    "editor.stickyScroll.enabled": false,
    "editor.occurrencesHighlight": "off",
    "editor.accessibilitySupport": "off",
    "editor.peekWidgetDefaultFocus": "editor",
    "editor.fontFamily": "FiraCode Nerd Font",
    "editor.hideCursorInOverviewRuler": true,
    "editor.suggestOnTriggerCharacters": false,
    "editor.scrollbar.verticalScrollbarSize": 12,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.guides.highlightActiveIndentation": false,
    "editor.find.autoFindInSelection": "multiline",
    "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
    },
    "explorer.openEditors.visible": 1,
    "explorer.decorations.colors": false,
    "files.trimTrailingWhitespace": true,
    "search.mode": "reuseEditor",
    "vim.leader": " ",
    "vim.showcmd": false,
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.cursorStylePerMode.insert": "line",
    "workbench.colorCustomizations": {
        "[Darcula Contrast]": {
            "editor.background": "#292c33",
            "sideBar.background": "#2c2f37",
            "statusBar.border": "#1f2127",
            "statusBar.background": "#1f2127",
            "statusBar.foreground": "#c1c1c1",
            "tab.activeBackground": "#292C33",
            "tab.inactiveBackground": "#30343D",
            "editorGroupHeader.tabsBackground": "#30343D",
            "titleBar.activeBackground": "#30343D",
            "titleBar.inactiveBackground": "#30343D",
            "editor.lineHighlightBackground": "#2E323A",
            "panel.background": "#1f2127",
            "terminal.background": "#1f2127",
            "diffEditor.insertedTextBackground": "#343c2600"
        }
    },
    "editor.tokenColorCustomizations": {
        "numbers": "#B99770",
        "functions": "#7FABE5",
        "comments": "#5e6165",
        "[Darcula Contrast]": {
            "textMateRules": [
                {
                    "scope": "support.type.primitive.rust",
                    "settings": {
                        "foreground": "#7cb0bc"
                    }
                },
                {
                    "scope": "variable.other.property",
                    "settings": {
                        "foreground": "#c47978"
                    }
                },
                {
                    "scope": [
                        "entity.name.type.class",
                        "entity.name.type.struct.rust",
                        "entity.name.type.trait.rust",
                        "entity.name.type.enum.rust"
                    ],
                    "settings": {
                        "foreground": "#7cb0bc"
                    }
                },
                {
                    "scope": "keyword.other.rust",
                    "settings": {
                        "foreground": "#ab7bcb"
                    }
                },
            ]
        }
    },
    "editor.find.seedSearchStringFromSelection": "selection",
    "files.eol": "\n",
    "files.hotExit": "off",
    "window.restoreWindows": "none",
    "workbench.editor.decorations.colors": false,
    "remote.SSH.remotePlatform": {
        "csslab12.uwb.edu": "linux"
    },
    "cmake.configureOnOpen": false,
    "git.confirmSync": false,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.mutable": {
                "underline": false,
            }
        }
    },
    "rust-analyzer.lens.enable": false,
    "rust-analyzer.lens.run.enable": false,
    "rust-analyzer.hover.actions.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.completion.autoimport.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "problems.showCurrentInStatus": true,
    "explorer.autoReveal": false,
    "search.exclude": {
        "**/.cache/**": true,
        "**/build": true,
        "**/target": true
    },
    "extensions.ignoreRecommendations": true,
    "window.titleBarStyle": "custom",
    "security.workspace.trust.untrustedFiles": "open",
    "window.title": "${activeFolderShort}/${activeEditorShort}${separator}${rootName}${separator}${profileName}",
    "search.showLineNumbers": true,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "workbench.editorAssociations": {
        "*.nes": "hexEditor.hexedit"
    },
    "scm.diffDecorations": "gutter",
    "workbench.layoutControl.enabled": false,
    "cmake.showOptionsMovedNotification": false,
    "problems.decorations.enabled": false,
    "editor.minimap.maxColumn": 80,
    "window.commandCenter": false,
    "editor.minimap.enabled": false,
    "editor.indentSize": "tabSize",
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "[typescriptreact]": {
        "editor.tabSize": 2
    },
    "diffEditor.hideUnchangedRegions.enabled": true,
    "git.ignoreLegacyWarning": true,
    "editor.scrollbar.horizontal": "hidden",
    "breadcrumbs.enabled": false,
}