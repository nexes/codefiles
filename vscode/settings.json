{
  "workbench.startupEditor": "none",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.restoreViewState": false,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "vscode-seedling-icon-theme",
  "workbench.tree.indent": 14,
  "workbench.commandPalette.history": 5,
  "workbench.colorTheme": "Nord",
  "workbench.activityBar.visible": true,
  "zenMode.hideTabs": false,
  "diffEditor.renderSideBySide": false,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "type",
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "search.useReplacePreview": false,
  "search.useIgnoreFiles": true,
  "search.showLineNumbers": true,
  "search.searchOnType": true,
  "editor.autoClosingBrackets": "always",
  "editor.tabSize": 4,
  "editor.folding": false,
  "editor.selectionHighlight": false,
  "editor.wordBasedSuggestions": false,
  "editor.snippetSuggestions": "none",
  "editor.suggestOnTriggerCharacters": true,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false,
  },
  "editor.parameterHints.enabled": true,
  "editor.cursorBlinking": "solid",
  "editor.fontLigatures": true,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.minimap.enabled": false,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.renderLineHighlight": "all",
  "editor.find.autoFindInSelection": "multiline",
  "editor.overviewRulerBorder": false,
  "editor.suggest.showSnippets": false,
  "editor.suggestSelection": "first",
  "editor.lightbulb.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.hideCursorInOverviewRuler": true,
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.title": "${activeEditorMedium}",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "git.decorations.enabled": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.hotExit": "off",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*/__pycache__": true,
    "**/node_modules": true,
    "**/target/debug": true,
    "**/target/rls": true,
    "**/.rustup": true,
    "**/.classpath": true,
    "**/*.class": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "search.exclude": {
    "**/__pycache__": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.rustup": true,
    "**/*.map": true,
    "**/bower_components": true,
    "**/build": true,
    "**/ios/build": true,
    "**/node_modules": true,
    "**/private": true,
    "**/target/debug": true,
    "**/target/rls": true,
    "**/typings": true,
    "**/var": true
  },
  "[typescript]": {
    "editor.tabSize": 2,
  },
  "[java]": {
    "editor.insertSpaces": true,
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.tabSize": 2,
  },
  "[python]": {
    "editor.insertSpaces": true
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[cpp]": {
    "editor.hover.enabled": false,
    "editor.wordBasedSuggestions": false,
    "editor.suggest.insertMode": "replace",
    "editor.semanticHighlighting.enabled": true
  },
  "diffEditor.ignoreTrimWhitespace": true,
  "window.menuBarVisibility": "toggle",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "extensions.ignoreRecommendations": false,
  "vim.easymotion": false,
  "vim.leader": ",",
  "vim.useSystemClipboard": true,
  "vim.history": 1,
  "vim.useCtrlKeys": false,
  "git.untrackedChanges": "separate",
  "problems.showCurrentInStatus": true,
  "cmake.configureOnOpen": false,
  "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: Google, UseTab: Never, IndentWidth: 4, TabWidth: 4, Cpp11BracedListStyle: true, BreakBeforeBraces: Custom, BraceWrapping: {AfterFunction: true, AfterClass: true, AfterStruct: true, BeforeCatch: true, AfterControlStatement: false}, AllowShortIfStatementsOnASingleLine: true, NamespaceIndentation: All , IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4}",
  "C_Cpp.clang_format_style": "{BasedOnStyle: Google, UseTab: Never, IndentWidth: 4, TabWidth: 4, Cpp11BracedListStyle: true, BreakBeforeBraces: Custom, BraceWrapping: {AfterFunction: true, AfterClass: true, AfterStruct: true, BeforeCatch: true, AfterControlStatement: false}, AllowShortIfStatementsOnASingleLine: true, NamespaceIndentation: All , IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4}",
  "C_Cpp.default.cppStandard": "c++17",
  "breadcrumbs.filePath": "off",
  "breadcrumbs.enabled": false,
  "workbench.colorCustomizations": {
    "[Default Dark+]": {
      "statusBar.background": "#252525",
      "statusBar.foreground": "#c4c4c4",
      "activityBar.background": "#252525",
    },
    "[September Steel]": {
      "editor.selectionBackground": "#12557c",
      "editorCursor.foreground": "#3f6981",
      "editor.findMatchHighlightBackground": "#1b5475",
      "editorSuggestWidget.selectedBackground": "#12557c"
    },
    "[Hybrid Next (Gray Background)]": {
      "statusBar.background": "#3f3f3f"
    },
    "[Nord]": {
      "editor.selectionBackground": "#12557c",
      "editorSuggestWidget.selectedBackground": "#12557c",
      "editorBracketMatch.background": "#576674",
      "editorBracketMatch.border": "#576674",
      "peekViewEditor.background": "#222a35",
      "peekViewResult.background": "#222a35",
      "peekViewTitle.background": "#141518",
      "peekView.border": "#141518",
      "list.activeSelectionBackground": "#576674",
      "list.activeSelectionForeground": "#d0d6db",
      "list.focusBackground": "#576674"
    }
  },
  "workbench.statusBar.visible": true,
  "window.zoomLevel": 0,
  // "terminal.integrated.rendererType": "experimentalWebgl",
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "rust-analyzer.hoverActions.enable": false,
  "rust-analyzer.lens.enable": false,
  "C_Cpp.updateChannel": "Insiders",
}
