{
    "telemetry.telemetryLevel": "error",
    "workbench.tree.indent": 16,
    "workbench.startupEditor": "none",
    "workbench.editor.restoreViewState": false,
    "workbench.tree.renderIndentGuides": "none",
    "workbench.iconTheme": "ayu-mirage-zed",
    "editor.folding": false,
    "editor.fontSize": 14,
    "editor.lineHeight": 1.6,
    "editor.wordWrapColumn": 90,
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.matchBrackets": "near",
    "editor.cursorBlinking": "solid",
    "editor.lightbulb.enabled": "off",
    "editor.detectIndentation": false,
    "editor.guides.indentation": false,
    "editor.trimAutoWhitespace": false,
    "editor.snippetSuggestions": "none",
    "editor.overviewRulerBorder": false,
    "editor.occurrencesHighlight": "off",
    "editor.accessibilitySupport": "off",
    "editor.peekWidgetDefaultFocus": "editor",
    "editor.fontFamily": "FiraCode Nerd Font",
    "editor.hideCursorInOverviewRuler": true,
    "editor.suggestOnTriggerCharacters": false,
    "editor.scrollbar.verticalScrollbarSize": 12,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.guides.highlightActiveIndentation": false,
    "editor.find.autoFindInSelection": "multiline",
    "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
    },
    "explorer.openEditors.visible": 1,
    "files.trimTrailingWhitespace": true,
    "search.mode": "reuseEditor",
    "vim.leader": " ",
    "vim.showcmd": false,
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.cursorStylePerMode.insert": "line",
    "editor.find.seedSearchStringFromSelection": "selection",
    "files.eol": "\n",
    "files.hotExit": "off",
    "window.restoreWindows": "none",
    "remote.SSH.remotePlatform": {
        "csslab12.uwb.edu": "linux"
    },
    "cmake.configureOnOpen": false,
    "git.confirmSync": false,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.mutable": {
                "underline": false,
            }
        }
    },
    "workbench.colorCustomizations": {
        "[Ayu Mirage Zed]": {
            "editor.background": "#2a2d3b",
            "focusBorder": "#2a2d3b00",
            "list.activeSelectionBackground": "#5b5f74",
            "list.focusBackground": "#5b5f74",
            "list.inactiveSelectionBackground": "#5b5f74",
            "list.inactiveSelectionForeground": "#cdd4f1",
            "list.activeSelectionForeground": "#cdd4f1",
            "tab.border": "#54565d00",
            "tab.activeForeground": "#c3c7d8",
            "panel.background": "#252935",
            "quickInput.foreground": "#ADB5D5",
            "quickInput.background": "#292C3B",
            "quickInputList.focusForeground": "#c3c7d8",
            "sideBar.border": "#363944",
            "sideBar.foreground": "#ADB5D5",
            "sideBar.background": "#252935",
            "titleBar.activeBackground": "#232633",
            "titleBar.border": "#232633",
            "statusBar.background": "#232633",
            "statusBar.border": "#232633",
            "sideBarStickyScroll.border": "#ADB5D5",
            "panelStickyScroll.border": "#ADB5D5",
            "editorCursor.foreground": "#dbc4bf",
            "activityBar.border": "#252935",
            "activityBar.background": "#252935",
        },
    },
    "editor.tokenColorCustomizations": {
        "[Ayu Mirage Zed]": {
            "strings": "#cde198",
            "functions": "#dbc38a",
            "keywords": "#B395D1",
            "numbers": "#E4A47B",
            "variables": "#d0d5ed",
            "textMateRules": [
                {
                    "scope": "variable.parameter",
                    "settings": {
                        "foreground": "#d0d5ed"
                    }
                }
            ]
        }
    },
    "rust-analyzer.lens.enable": false,
    "rust-analyzer.lens.run.enable": false,
    "rust-analyzer.hover.actions.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.completion.autoimport.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "problems.showCurrentInStatus": true,
    "explorer.autoReveal": false,
    "search.exclude": {
        "**/.cache/**": true,
        "**/build": true,
        "**/target": true
    },
    "extensions.ignoreRecommendations": true,
    "window.titleBarStyle": "custom",
    "security.workspace.trust.untrustedFiles": "open",
    "window.title": "${activeFolderShort}/${activeEditorShort}${separator}${rootName}${separator}${profileName}",
    "search.showLineNumbers": true,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "workbench.editorAssociations": {
        "*.nes": "hexEditor.hexedit"
    },
    "scm.diffDecorations": "gutter",
    "workbench.layoutControl.enabled": false,
    "cmake.showOptionsMovedNotification": false,
    "problems.decorations.enabled": false,
    "editor.minimap.maxColumn": 80,
    "window.commandCenter": false,
    "editor.minimap.enabled": false,
    "editor.indentSize": "tabSize",
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "[typescriptreact]": {
        "editor.tabSize": 2
    },
    "diffEditor.hideUnchangedRegions.enabled": true,
    "git.ignoreLegacyWarning": true,
    "editor.scrollbar.horizontal": "hidden",
    "workbench.colorTheme": "Ayu Mirage Zed",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "editor.stickyScroll.enabled": false,
    "breadcrumbs.filePath": "last",
    "breadcrumbs.icons": false,
    "breadcrumbs.symbolPath": "last",
    "breadcrumbs.enabled": false,
    "workbench.editor.highlightModifiedTabs": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.hover.enabled": false,
}